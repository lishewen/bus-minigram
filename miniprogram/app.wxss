/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

page {
  background-color: #FFED8F;
  background-repeat: round;
  background-attachment: fixed;
  opacity: 0.9;
}

.route-info {
  font-size: 12px;
  color: #8e8e93;
  margin-top: 4px;
  display: flex;
}

.route-info view {
  margin-right: 8px;
}

.tabbar-item {
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #929292;
  font-size: 10px;
}

.tabbar-item image {
  width: 20px;
  height: 20px;
  margin-bottom: 4px;
}

.rightarrow {
  color: #8d8d8d;
  margin-left: 4px;
  margin-right: 4px;
}

.txtrouteName {
  font-size: 18px;
  color: #4a4a4a;
}

.noinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #000000;
  font-size: 14px;
  margin-top: -64px;
  padding-top: 50%;
}

.noinfoimg {
  width: 64px;
  height: 64px;
  margin-bottom: 8px;
}
